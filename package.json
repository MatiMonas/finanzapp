{
  "name": "finanzapp",
  "version": "1.0.0",
  "description": "",
  "main": "app.ts",
  "scripts": {
    "start": "NODE_PATH=./build node build/app.js",
    "dev": "env-cmd -f .env.local nodemon -L --inspect=0.0.0.0:9229 --signal SIGINT --nolazy src/app.ts",
    "dev:prod": "env-cmd -f .env nodemon -L --inspect=0.0.0.0:9229 --signal SIGINT --nolazy src/app.ts",
    "build": "npx rimraf ./build && npx prisma generate  && tsc -p tsconfig.build.json",
    "test": "dotenv -e .env.test -- bun test",
    "coverage": "dotenv -e .env.test -- bun test --coverage",
    "setup": "./setup.sh",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "env-cmd -f .env.local prisma migrate dev --create-only",
    "prisma:migrate:prod": "env-cmd -f .env prisma migrate dev --create-only",
    "prisma:deploy": "env-cmd -f .env prisma migrate deploy",
    "prisma:deploy:local": "env-cmd -f .env.local prisma migrate deploy",
    "prisma:deploy-test": "dotenv -e .env.test -- prisma migrate deploy",
    "db:setup:local": "env-cmd -f .env.local prisma db push && env-cmd -f .env.local prisma generate",
    "db:setup:test": "env-cmd -f .env.test prisma db push && env-cmd -f .env.test prisma generate",
    "db:setup:prod": "env-cmd -f .env prisma db push && env-cmd -f .env prisma generate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/moment": "^2.13.0",
    "@types/morgan": "^1.9.9",
    "@types/mysql": "^2.15.21",
    "@types/node": "^18.11.9",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/yamljs": "^0.2.34",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "dotenv-cli": "^7.3.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.32.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.7",
    "lint-staged": "^16.1.2",
    "nodemon": "^3.1.0",
    "prisma": "^5.22.0",
    "rimraf": "^3.0.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.1",
    "ts-prune": "^0.10.3",
    "tsconfig-paths": "^4.1.0",
    "typescript": "5.1.3"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "bunx eslint --fix",
      "bunx prettier --write"
    ]
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "redis": "4.6.8",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tsoa": "^6.6.0",
    "yamljs": "^0.3.0",
    "zod": "^3.23.8"
  }
}
