{
  "name": "finanzapp",
  "version": "1.0.0",
  "description": "",
  "main": "app.ts",
  "scripts": {
    "start": "NODE_PATH=./build node build/app.js",
    "dev": "nodemon -L --inspect=0.0.0.0:9229 --signal SIGINT --nolazy src/app.ts",
    "build": "npx rimraf ./build && npx prisma generate && npx prisma generate --schema=prisma/platform.prisma && tsc -p tsconfig.build.json",
    "test": "env-cmd -f .env.test jest",
    "coverage": "env-cmd -f .env.test npx jest --coverage",
    "setup": "./setup.sh",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --create-only",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:deploy-test": "dotenv -e .env.test -- prisma migrate deploy",
    "test:unit": "jest ./tests/unit",
    "test:integration": "dotenv -e .env.test -- jest ./tests/integration"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/mysql": "^2.15.21",
    "@types/node": "^18.11.9",
    "@types/supertest": "^6.0.2",
    "dotenv-cli": "^7.3.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.7",
    "nodemon": "^3.1.0",
    "prisma": "^5.19.0",
    "rimraf": "^3.0.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "5.1.3"
  },
  "dependencies": {
    "@prisma/client": "^5.19.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "redis": "4.6.8",
    "zod": "^3.21.4"
  }
